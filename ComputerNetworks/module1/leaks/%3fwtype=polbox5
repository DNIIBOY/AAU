<script type="text/javascript">
var dp1 = new DatePicker({
relative:'inDateFFlight',
externalControl:'from_dp_icon',
keepFieldEmpty:false,
topOffset:21,
leftOffset:-1,
afterClose:testfrom,
dateFilter:DatePickerUtils.noDatesBefore(0)
});
var dp2 = new DatePicker({
relative:'outDateFFlight',
externalControl:'to_dp_icon',
keepFieldEmpty:false,
topOffset:21,
leftOffset:-1,
afterClose:testto,
dateFilter:DatePickerUtils.noDatesBefore(0)
});
function testfrom(){
var from_arr = $('inDateFFlight').value.split("-");
var to_arr = $('outDateFFlight').value.split("-");
from = new Date(from_arr[0],from_arr[1]-1,from_arr[2]);
to = new Date(to_arr[0],to_arr[1]-1,to_arr[2]);
if(from>to){
$('outDateFFlight').value=$('inDateFFlight').value;
}
}
function testto(){
var from_arr = $('inDateFFlight').value.split("-");
var to_arr = $('outDateFFlight').value.split("-");
from = new Date(from_arr[0],from_arr[1]-1,from_arr[2]);
to = new Date(to_arr[0],to_arr[1]-1,to_arr[2]);
if(from>to){
alert("Hjemrejsedato kan ikke være før afrejsedato.\nHjemrejsedato er nu sat til afrejsedato.");
$('outDateFFlight').value=$('inDateFFlight').value;
}
}
</script>
</div>
