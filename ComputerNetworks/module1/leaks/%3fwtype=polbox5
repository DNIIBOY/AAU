<script type="text/javascript" src="http://tgt.dk/template/v1-0/components/js/autocomplete.js"></script>
<script type="text/javascript">
clearBoth = function(el,hidden){
el.value="";
$(hidden).value="";
}
Event.observe(window, 'load', function() {
new Autocomplete('fromFlight', { serviceUrl:'/?json=iatacodes',
minChars:2, maxHeight:400,
mode:'flight',
width:260,
onSelect: function(value, data){
$("from_ac_flight").value=data;
}
});
new Autocomplete('destinationFlight', { serviceUrl:'/?json=iatacodes',
minChars:2,
maxHeight:400,
mode:'flight',
width:260,
onSelect: function(value, data){
$("destination_ac_flight").value=data;
}
});
});
</script>
