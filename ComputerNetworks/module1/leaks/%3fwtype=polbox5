});
addTracking = function(){
$('main').select('a').each(function(el){
var url = el.readAttribute('href');
if (url.lastIndexOf("?") == -1){
url+="?t=pol";
}else{
url+="&t=pol"
}
el.writeAttribute('href',url);
});
}
